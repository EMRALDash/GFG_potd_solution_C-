class Solution {
  public:
    vector<string> binstr(int n) {
        vector<string> result;
        int total = 1 << n; // 2^n combinations

        for (int i = 0; i < total; i++) {
            string binary = "";
            for (int j = n - 1; j >= 0; j--) {
                binary += ((i >> j) & 1) ? '1' : '0';
            }
            result.push_back(binary);
        }

        return result;
    }
};
